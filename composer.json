{
    "name": "xiaohuilam/searching",
    "description": "Laravel 顶级搜索功能",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "xiaohui.lam",
            "email": "xiaohui.lam@icloud.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Searching\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Searching\\Tests\\": "tests/",
            "App\\": "publishes/app/"
        }
    },
    "require": {
        "php": ">7.0.0",
        "laravel/framework": ">=5.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "orchestra/testbench": "~3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Searching\\Providers\\SearchingServiceProvider"
            ]
        }
    }
}
