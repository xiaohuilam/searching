{
    "name": "xiaohuilam/searching",
    "description": "Laravel 顶级搜索功能",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "xiaohui.lam",
            "email": "xiaohui.lam@icloud.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Searching\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Searching\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">7.0.0",
        "laravel/framework": "5.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Searching\\Providers\\SearchingServiceProvider"
            ]
        }
    }
}
